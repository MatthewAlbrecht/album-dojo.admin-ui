{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "port": 9222,
      "url": "http://localhost:1333",
      "webRoot": "${workspaceFolder}/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      },
      "runtimeArgs": [
        "--remote-debugging-port=9222",
        "--user-data-dir=developer"
      ]
    }
  ]
}
